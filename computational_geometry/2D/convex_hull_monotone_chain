#include <bits/stdc++.h>
using namespace std;

typedef long long ll;

struct point {
	double x, y;

	bool operator <(const point &p) const {
		return x < p.x || (x == p.x && y < p.y);
	}
};

double cross(point &a, point &b, point &c)
{
	return (b.x - a.x) * (c.y - a.y) - (b.y - a.y) * (c.x - a.x);
}

vector<point> convex_hull(vector<point> v) // counterclockwise
{
	ll n = v.size(), k = 0;
	if (n <= 3) return v;
	vector<point> nodes(2*n);

	sort(v.begin(), v.end());

	for (ll i = 0; i < n; i++) {
		while (k >= 2 && cross(nodes[k-2], nodes[k-1], v[i]) <= 0) k--;
		//kalo mau CW, <= ganti >
		nodes[k++] = v[i];
	}

	for (ll i = n-1, t = k+1; i > 0; i--) {
		while (k >= t && cross(nodes[k-2], nodes[k-1], v[i-1]) <= 0) k--;
		//kalo mau CW, <= ganti >
		nodes[k++] = v[i-1];
	}

	nodes.resize(k-1);
	return nodes;
}

int main(){
	vector <point> p;

	for (int i = 0; i < 7; i++){
		double x,y;
		cin >> x >> y;

		p.push_back ({x,y});
	}

	p = convex_hull (p);
	cout << endl;
	for (auto x : p) cout << x.x << " " << x.y << endl;
}
